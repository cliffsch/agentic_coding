{
  "name": "computer-use-orchestrator",
  "description": "Orchestrator for computer-use-v3 using Kimi K2.5 for coordination and oversight",
  "model": {
    "provider": "openrouter",
    "model_id": "moonshotai/kimi-k2.5",
    "temperature": 0.2,
    "max_tokens": 8192
  },
  "agent": {
    "mode": "orchestrator",
    "max_iterations": 50,
    "auto_approve_file_ops": true,
    "auto_approve_shell": [
      "ls",
      "cat",
      "grep",
      "find",
      "git status",
      "git log"
    ],
    "blocked_shell": [
      "rm -rf",
      "sudo",
      "git push"
    ]
  },
  "context": {
    "include_files": [
      "DESIGN.md",
      "IMPLEMENTATION.md",
      "HANDOFF.md",
      "PROGRESS.md"
    ],
    "working_directory": "skills/computer-use-v3"
  },
  "orchestration": {
    "implementation_profile": "computer-use-impl",
    "review_profile": "computer-use-review",
    "signal_files": {
      "start": "IMPLEMENTATION_START.md",
      "complete": "IMPLEMENTATION_COMPLETE.md",
      "approved": "REVIEW_APPROVED.md",
      "issues": "REVIEW_ISSUES.md",
      "blocked": "BLOCKED.md"
    }
  },
  "rules": [
    "Monitor signal files for handoff triggers",
    "Delegate implementation tasks to computer-use-impl profile",
    "Delegate review tasks to computer-use-review profile",
    "Track progress via PROGRESS.md",
    "Escalate to human if BLOCKED.md appears or max cycles exceeded",
    "Do not implement code directly - only coordinate",
    "Maintain master todo list for project tracking"
  ]
}
